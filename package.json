{
  "name": "opencashdao",
  "author": "Kuldeep Singh",
  "version": "0.1.0",
  "description": "OpenCashDAO",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "compile:controller": "cashc ./DAO/Controller.cash -o ./artifacts/Controller.json",
    "compile:proposal": "cashc ./DAO/Proposal.cash -o ./artifacts/Proposal.json",
    "compile:addThreads": "cashc ./DAO/executions/AddThreads.cash -o ./artifacts/executions/AddThreads.json",
    "compile:removeThreads": "cashc ./DAO/executions/RemoveThreads.cash -o ./artifacts/executions/RemoveThreads.json",
    "compile:replaceThreads": "cashc ./DAO/executions/ReplaceThreads.cash -o ./artifacts/executions/ReplaceThreads.json",
    "compile:failProposal": "cashc ./DAO/executions/FailProposal.cash -o ./artifacts/executions/FailProposal.json",
    "compile:voting": "cashc ./DAO/Voting.cash -o ./artifacts/Voting.json",
    "compile:upgradable": "cashc ./Upgradable/Upgradable.cash -o ./artifacts/Upgradable.json",
    "compile:contractA": "cashc ./Upgradable/ContractA.cash -o ./artifacts/ContractA.json",
    "compile:contractNew": "cashc ./Upgradable/ContractNew.cash -o ./artifacts/ContractNew.json",
    "compile": "npm run compile:controller && npm run compile:proposal && npm run compile:addThreads && npm run compile:removeThreads && npm run compile:replaceThreads && npm run compile:failProposal && npm run compile:voting && npm run compile:upgradable && npm run compile:contractA && npm run compile:contractNew",
    "build": "npm run compile && node index.js"
  },
  "dependencies": {
    "@bitauth/libauth": "^3.1.0-next.4",
    "cashc": "^0.11.0-next.2",
    "cashscript": "^0.11.0-next.2",
    "dotenv": "^16.4.7"
  }
}
